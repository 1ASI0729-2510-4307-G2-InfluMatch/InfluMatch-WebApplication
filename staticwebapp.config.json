{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/static/*", 
      "/images/*",
      "/favicon.ico",
      "/*.js",
      "/*.css",
      "/*.woff",
      "/*.woff2",
      "/*.ttf",
      "/*.eot",
      "/*.svg",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.gif",
      "/*.ico",
      "/*.json"
    ]
  },
  "routes": [
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/login",
      "serve": "/index.html", 
      "statusCode": 200
    },
    {
      "route": "/register",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/onboarding",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard/profile",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard/profile/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard/collaborations",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard/collaborations/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard/new-collaboration",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard/agenda",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard/chats",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard/chats/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
} 